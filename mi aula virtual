    require(['theme_boost/bootstrap/popover'], function() {
        var target = $("#calendar-day-popover-link-1-2021-328-619f58bab855c619f58bab85611");
        target.popover({
            content: function() {
                var source = target.next().find("> *:not('.hidden')");
